<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="4.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jtool.eclipse.handlers.ProjectAction"
            commandId="org.jtool.eclipse.commands.ProjectAction">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any">
         <separator
               name="org.jtool.eclipse.separator"
               visible="true">
         </separator>
         <command
               commandId="org.jtool.eclipse.commands.ProjectAction"
               label="Analyze Code with Jtool"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.jtool.eclipse.commands.category"
            name="Jtool Category">
      </category>
      <command
            categoryId="org.jtool.eclipse.commands.category"
            id="org.jtool.eclipse.commands.ProjectAction"
            name="Project Action">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.jtool.eclipse.Activator"></startup>
   </extension>
